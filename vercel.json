{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/provinsi",
      "dest": "/api/provinsi.js"
    },
    {
      "src": "/api/wisata",
      "dest": "/api/wisata.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
